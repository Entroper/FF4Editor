<Window x:Class="FF4MapEdit.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FF4MapEdit"
        mc:Ignorable="d"
        Title="FF4MapEdit" Height="500" Width="800">
	<Grid>
		<ToolBarTray HorizontalAlignment="Left" Height="30" VerticalAlignment="Top" Width="792">
			<ToolBar x:Name="toolBar" HorizontalAlignment="Left" Height="30" VerticalAlignment="Top">
				<Button x:Name="OpenButton" Click="OpenButton_Click">
					<Image Source="icons/open.png"/>
				</Button>
				<Button x:Name="SaveButton">
					<Image Source="icons/save.png"/>
				</Button>
			</ToolBar>
		</ToolBarTray>
	</Grid>
</Window>
